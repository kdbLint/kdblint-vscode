{
  "name": "kdblint-vscode",
  "version": "0.1.0",
  "publisher": "kdbLint",
  "engines": {
    "vscode": "^1.22.0"
  },
  "license": "MIT",
  "displayName": "kdbLint",
  "description": "kdbLint extension for Visual Studio Code",
  "categories": [
    "Formatters",
    "Linters",
    "Programming Languages"
  ],
  "keywords": [
    "q",
    "k",
    "kdb",
    "kdb+"
  ],
  "preview": true,
  "main": "index.js",
  "contributes": {
    "grammars": [
      {
        "language": "k",
        "scopeName": "source.k",
        "path": "./syntaxes/k.tmLanguage.json"
      },
      {
        "language": "q",
        "scopeName": "source.q",
        "path": "./syntaxes/q.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "k",
        "extensions": [
          ".k"
        ],
        "firstLine": "^#!.*\\bk\\b",
        "configuration": "./language-configuration.json"
      },
      {
        "id": "q",
        "extensions": [
          ".q"
        ],
        "aliases": [
          "kdb",
          "kdb+"
        ],
        "firstLine": "^#!.*\\bq\\b",
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "activationEvents": [
    "workspaceContains:**/*.{k,q}"
  ],
  "dependencies": {},
  "devDependencies": {},
  "extensionKind": [
    "workspace"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kdblint/kdblint-vscode.git"
  },
  "author": {
    "name": "David Strachan",
    "email": "david@kdblint.com"
  },
  "bugs": {
    "url": "https://github.com/kdblint/kdblint-vscode/issues"
  },
  "homepage": "https://github.com/kdblint/kdblint-vscode#readme"
}
